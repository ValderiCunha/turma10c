package repeticao;

import javax.swing.JOptionPane;

public class DesafioDo {

	public static void main(String[] args) {
		int jogador1 = Integer.parseInt(JOptionPane.showInputDialog("Digite um número inteiro"));
		int chute = 0;
		do {
			chute = Integer.parseInt(JOptionPane.showInputDialog("Adivinhe o número digitado pelo jogador 1"));
		}while(jogador1!=chute);
			
		

	}
}



